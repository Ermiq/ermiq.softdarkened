{
	"name": "SoftC#",
	"type": "dark",
	"colors": {
		"editor.background": "#111111",
		"editor.foreground": "#ccccccf3",
		"activityBar.background": "#363636",
		"activityBar.activeBackground": "#1b1b1b",
		"sideBar.background": "#1b1b1b",
		"sideBar.border": "#7e7e7e",
		"sideBarSectionHeader.background": "#353535",
		"activityBarBadge.background": "#0a96e7",
		"sideBarTitle.foreground": "#bbbbbb",
		"editor.selectionBackground": "#8a8a8a7c",
		"editor.selectionForeground": "#2600ffd7",
		"editor.selectionHighlightBackground": "#fcfcfc25",
		"editor.selectionHighlightBorder": "#fcfcfc36"
	},
	"tokenColors": [
		{
			"scope": [],
			"settings": {
				"foreground": "#60ee8b"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.quoted.docstring"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7f817e"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"entity.name.variable",
				"string constant.other.placeholder",
				"variable.other.object.property",
				"meta.function-call.arguments"
			],
			"settings": {
				"foreground": "#bbbbbbf1"
			}
		},
		{
			"name": "Objects",
			"scope": [
				"variable.other.object"
			],
			"settings": {
				"foreground": "#4f89d6f1"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ccccccf1"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370f1"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type"
			],
			"settings": {
				"foreground": "#4f89d6f1"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string",
				"constant.other.color",
				"punctuation"
			],
			"settings": {
				"foreground": "#4f89d6f1"
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"meta.preprocessor",
				"punctuation.separator.hash",
				"entity.name.variable.preprocessor",
				"keyword.preprocessor"
			],
			"settings": {
				"foreground": "#b85fcef1"
			}
		},
		{
			"name": "Control keywords",
			"scope": [
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#4f89d6f1"
			}
		},
		{
			"name": "Operators +, =, <, >, ++, ==, &, |",
			"scope": [
				"keyword.operator.logical",
				"keyword.operator.assignment",
				"keyword.operator.comparison",
				"keyword.operator.increment",
				"keyword.operator.relational",
				"keyword.operator.conditional",
				"keyword.operator.bitwise"
			],
			"settings": {
				"foreground": "#ce6666f1"
			}
		},
		{
			"name": "Keywords if, else, for, foreach, while",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#56ac53f1"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"meta.tag",
				"entity.name.tag",
				"entity.name.tag.localname",
				"variable.language.special"
			],
			"settings": {
				"foreground": "#ce6666f1"
			}
		},
		{
			"name": "Tags: attribute name",
			"scope": [
				"entity.name.type",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#bbbbbbf1"
			}
		},
		{
			"name": "Function names, Special Methods",
			"scope": [
				"entity.name.function",
				"meta.function-call.generic",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#d3964cf1",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178f1"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"keyword.other",
				"constant.language",
				"storage.modifier",
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#ce6666f1"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#bbbbbbf1"
			}
		},
		{
			"name": "Strings, Symbols, Inherited Class",
			"scope": [
				"string.quoted.single",
				"string.quoted.double",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d3964cf1"
			}
		},
		{
			"name": "Support type",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#288be7f1"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFCB6Bf1"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#C3E88Df1"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#FF5370f1"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#C792EAf1"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#d19292f1"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		}
	]
}
